{"cells":[{"cell_type":"code","execution_count":null,"metadata":{},"outputs":[],"source":['import pandas as pd', 'import numpy as np', 'from sklearn.feature_extraction.text import TfidfVectorizer', 'from sklearn.linear_model import LogisticRegression', 'from sklearn.metrics.pairwise import cosine_similarity', 'import PyPDF2', 'import docx', '', '# Load dataset', 'job_data = pd.read_csv("data/job_descriptions.csv")', '', '# Sample function to parse resumes (PDF and Word)', 'def parse_pdf(pdf_file):', '    with open(pdf_file, "rb") as f:', '        reader = PyPDF2.PdfReader(f)', '        text = ""', '        for page in reader.pages:', '            text += page.extract_text()', '    return text', '', 'def parse_docx(docx_file):', '    doc = docx.Document(docx_file)', '    text = ""', '    for para in doc.paragraphs:', '        text += para.text', '    return text', '', '# Function to preprocess and match resumes to job descriptions', 'def match_resume_to_job(resume_text, job_desc_texts):', '    vectorizer = TfidfVectorizer(stop_words="english")', '    tfidf_matrix = vectorizer.fit_transform([resume_text] + job_desc_texts)', '    cosine_sim = cosine_similarity(tfidf_matrix[0:1], tfidf_matrix[1:])', '    return cosine_sim', '', '# Example of matching a resume to job descriptions', 'resume_text = parse_pdf("data/sample_resume.pdf")  # Replace with actual resume path', "job_desc_texts = job_data['job_description'].tolist()  # Assuming a 'job_description' column", 'cosine_similarities = match_resume_to_job(resume_text, job_desc_texts)', '', '# Rank and display top matches', 'def rank_and_display(cosine_similarities):', "    job_data['match_score'] = cosine_similarities[0]", '    job_data = job_data.sort_values(by="match_score", ascending=False)', "    print(job_data[['job_title', 'match_score']].head())", '', 'rank_and_display(cosine_similarities)']}], "metadata":{}, "nbformat":4, "nbformat_minor":2}